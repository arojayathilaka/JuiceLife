<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.myapplication.CardPayments" android:label="Card Payments" android:parentActivityName="com.example.myapplication.PaymentMethod"></activity>
        <activity android:name="com.example.myapplication.CashOnDelivery" android:label="Cash On Delivery" android:parentActivityName="com.example.myapplication.PaymentMethod"/>
        <activity android:name="com.example.myapplication.PaymentMethod">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>